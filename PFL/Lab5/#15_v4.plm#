; Write a function to test the equality of two sets, without using the difference of two sets

(DEFUN solve(a b)
  (COND
    ((NULL b) 1)
    ((= (solve a (CDR b)) 0) 0)
    ((= (LENGTH a) (LENGTH (ADJOIN (CAR b) a))) 1)
    (T 0)
  )
)

(SETQ a (LIST 1 2 3))
(SETQ b '(1 3 2 9 ))
(SETQ res (SOLVE a b))
(WRITE res)
