(DEFUN sum(l)
  (COND
    ((NULL l) 0)
    ((NUMBER (CAR l)) (+ (CAR l) (sum (CDR l))))
    ((LISTP (CAR l)) (+ (sum(CAR l)) (sum(CDR l)))) 
  )

